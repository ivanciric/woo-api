{
    "version": 2,
    "builds": [
      {
        "src": "/api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      }
    ],
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs20.x",
        "memory": 3008,
        "maxDuration": 10
      }
    }
  }
  